<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SecureDefaults.AspNet</id>
    <version>0.0.2-alpha</version>
    <title>Secure configuration defaults for ASP.NET</title>
    <authors>bbowyersmyth</authors>
    <owners>bbowyersmyth</owners>
    <licenseUrl>http://www.opensource.org/licenses/apache2.0</licenseUrl>
    <projectUrl>http://github.com/bbowyersmyth/securedefaults</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Secure configuration defaults for ASP.NET</description>
    <summary>ASP.NET secure configuration defaults.</summary>
    <releaseNotes>
      Initial release
    </releaseNotes>
    <tags>ASP.NET WebForms Secure Defaults</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="AntiXSS" version="4.2.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Tools\*.*" target="tools" />
    <file src="Content\*.*" target="content" />
    <file src="Content\net40\*.*" target="content\net40" />
    <file src="Content\GenericError.html" target="content\net40\GenericError.html" />
    <file src="Content\web.Release.config.transform" target="content\net40\web.Release.config.transform" />
    <file src="Content\robots.txt" target="content\net40\robots.txt" />
    <file src="Content\net45\*.*" target="content\net45" />
    <file src="Content\GenericError.html" target="content\net45\GenericError.html" />
    <file src="Content\web.Release.config.transform" target="content\net45\web.Release.config.transform" />
    <file src="Content\robots.txt" target="content\net45\robots.txt" />
    <file src="..\HttpModules\bin\Release\SecureDefaults.dll" target="lib" />
  </files>
</package>
